{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "assignees": ["cascer1"],
    "rebaseWhen": "conflicted",
    "customManagers": [
      {
        "customType": "regex",
        "managerFilePatterns": ["home/.chezmoidata/cargo.yaml"],
        "matchStrings": [
          "^\\s+(?<depName>\\w+)\\s*:\\s*'(?<currentValue>[^'].+)'"
        ],
        "depNameTemplate": "cargo crate ${depName}",
        "datasourceTemplate": "crate"
    }
  ]
}

